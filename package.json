{
  "name": "return-deep-diff",
  "version": "0.1.1",
  "description": "Small library who deeply check for difference between two objects",
  "repository": "dispix/deep-diff",
  "main": "index.js",
  "scripts": {
    "build": "standard && babel src/deepDiff.js -o dist/main.js",
    "test": "standard && jest --verbose",
    "format": "standard --fix"
  },
  "standard": {
    "globals": [
      "expect",
      "it",
      "describe"
    ],
    "ignore": [
      "/dist",
      "/node_modules"
    ]
  },
  "babel": {
    "presets": "es2015"
  },
  "keywords": [
    "javascript",
    "object",
    "es6",
    "es2015"
  ],
  "author": "Octave \"Dispix\" Raimbault <dispyx@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "jest": "^15.1.1"
  }
}
